{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "standard",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {},
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "globals": {
        "google": "readonly",
        "maps": "readonly",
        "LatLng": "readonly",
        "LatLngLiteral": "readonly",
        "MapOptions": "readonly",
        "MapProps": "readonly",
        "MapComponent": "readonly"
    }
}